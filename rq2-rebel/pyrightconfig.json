{
  "venvPath": ".", // where to look for virtual environments
  "venv": ".venv", // the name of the env (not the full path)
  "typeCheckingMode": "basic", // lighter type checks
  "useLibraryCodeForTypes": true,
  "exclude": ["**/__pycache__", "**/.mypy_cache", "**/node_modules"],
  "extraPaths": ["./"]
}
